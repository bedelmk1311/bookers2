<h1>仮New book</h1>
<%= form_with model: @book do |f| %>
  <h4>画像用</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Opinion</h4>
  <%= f.text_area :body %>

  <p><%= f.submit 'Create Book' %></p>
<% end %>